{
  "extends": [
    "config:base"
  ],
  "gradle": {
    "enabled": true
  },
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch"
    }
  ]
}
